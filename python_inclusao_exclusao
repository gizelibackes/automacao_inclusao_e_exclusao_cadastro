import selenium
import pyautogui
import pyperclip
import time

#bibliotecas para automatizar consulta 
from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.common.alert import Alert

#biblioteca para delay de tempo
from time import sleep
from selenium.webdriver.support.wait import WebDriverWait
from selenium.webdriver.support.select import Select
from selenium.webdriver.common.action_chains import ActionChains
from selenium.webdriver.support.wait import WebDriverWait
from selenium.webdriver.support import expected_conditions as EC
from selenium.common.exceptions import TimeoutException

navegador = webdriver.Safari()
navegador.get("https://www.amigonaosecompra.com.br")

time.sleep(2)

navegador.maximize_window()

#busca botao ok para cookies
navegador.find_element('xpath','/html/body/main/div[11]/div/label').click()
time.sleep(3)

#busca botao quero ajudar
navegador.find_element('xpath','/html/body/div[3]/div/span').click()
time.sleep(3)

#fecha janela quero ajudar
navegador.find_element('xpath','/html/body/div[4]/div[2]/div/div/div/span/div/div[6]/div/div/div[2]').click()
time.sleep(3)

navegador.find_element(By.LINK_TEXT,'Entrar').click()
time.sleep(2)

#faz login
navegador.find_element('xpath','/html/body/div/div/div/div[2]/form/div[1]').send_keys('animaliza21@gmail.com', Keys.TAB)
time.sleep(1)
navegador.find_element('xpath','/html/body/div/div/div/div[2]/form/div[2]').send_keys('XXXXXXXX', Keys.ENTER)
time.sleep(2)

#APAGAR AS POSTAGENS
for i in range(27):
    
    #ATUALIZA PAGINA
    navegador.refresh()
    time.sleep(10)
    
    #ENCONTRA O PRIMEIRO POST E ACESSA 
    navegador.find_element('xpath','//*[@id="grid-pet"]/div/div[1]/a').send_keys(Keys.ENTER)
    time.sleep(10)

    #CLICA EM EDITAR
    navegador.find_element('xpath','/html/body/main/div[2]/div[2]/div[1]/div[1]/div/a[1]').send_keys(Keys.ENTER)
    time.sleep(10)

    #CLICA EM APAGAR ANIMAL 
    navegador.find_element('xpath','/html/body/main/div[2]/div[5]/a').send_keys(Keys.ENTER)
    time.sleep(2)

    #CONFIRMA EXCLUSAO
    navegador.switch_to.alert.accept()
    time.sleep(2)
    pyautogui.mouseDown()
    time.sleep(2)

    #ATUALIZA PAGINA
    navegador.refresh()
    time.sleep(10)

#CADASTRA ANIMAIS
for i in range(28):
    
    #BELA
    if i == 0:
        
        navegador.find_element('xpath','/html/body/header/nav/div[1]/div/div[3]/div/div[1]/label/img').click()
        time.sleep(2)
        
        navegador.find_element('xpath','//*[@id="user-menu"]/div[2]/a[1]').click()
        time.sleep(2)
        
        navegador.find_element(By.CSS_SELECTOR,'#pet-form_name').send_keys('Bela')
        time.sleep(1)
        
        especie = Select(navegador.find_element(By.CSS_SELECTOR,'#pet-form_specie'))
        time.sleep(1)
        #selecionando pelo texto visivel
        especie.select_by_visible_text('Cachorro')
        time.sleep(1)

        sexo = Select(navegador.find_element(By.CSS_SELECTOR,'#pet-sex'))
        time.sleep(1)
        #selecionando pelo texto visivel
        sexo.select_by_value('female')
        time.sleep(1)
        
        idade = Select(navegador.find_element(By.CSS_SELECTOR,'#pet-form_age'))
        time.sleep(1)
        #selecionando pelo texto visivel
        idade.select_by_value('adult')
        time.sleep(1)
        
        tamanho = Select(navegador.find_element(By.CSS_SELECTOR,'#pet-form_size'))
        time.sleep(1)
        #selecionando pelo texto visivel
        tamanho.select_by_value('big')
        time.sleep(1)
        
        pyautogui.press('DOWN', presses=10)
        time.sleep(1)
        
        navegador.find_element(By.ID,'pet-form_description').send_keys(" ")
        time.sleep(1)
        
        texto = f"""
IDADE: 3 ANOS
RAÇA: SRD 
PORTE: GRANDE

